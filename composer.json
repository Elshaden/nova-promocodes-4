{
    "name": "aberbin96/nova-promocodes-4",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
           "type": "vcs",
           "url": "https://github.com/Elshaden/laravel-promocodes.git"
        }
    ],
    "require": {
        "php": ">=7.3|8.1",
        "zgabievi/laravel-promocodes": "dev-L10"
    },
    "autoload": {
        "psr-4": {
            "Zorb\\NovaPromocodes\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Zorb\\NovaPromocodes\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
